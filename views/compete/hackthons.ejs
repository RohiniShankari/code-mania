<%layout("/layouts/boilerplate")%>

<h1 style="text-align:center;">Hackerearth hackthons</h1>
<div class="hackthons-page">
    
    <% hackerearthHackthons.forEach(contest => { %>
        <%if(contest.title!=null){%>
        <div class="card" style="width: 18rem;">
            <div class="card-body">
              <h5 class="card-title"><%=contest.title%></h5>
              <h6 class="card-subtitle mb-2 text-body-secondary"><%=contest.date%></h6>
              
              <a href=<%=contest.link%> class="btn btn-primary">Apply</a>
              
            </div>
          </div>
          <%}%>
    <%});%>
</div>

<h1 style="text-align:center;">Devfolio hackthons</h1>
<div class="hackthons-page">
    
    <% devfolioHackthons.forEach(contest => { %>
        <%if(contest.title!=null){%>
