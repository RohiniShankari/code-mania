              <h6 class="card-subtitle mb-2 text-body-secondary"><%=contest.date%></h6>
              
              <a href=<%=contest.link%> class="btn btn-primary">Apply</a>
              
            </div>
          </div>
          <%}%>
    <%});%>
</div>